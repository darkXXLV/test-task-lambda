{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "ModifyBucketPolicy",
        "Action": [
          "s3:GetBucketPolicy",
          "s3:PutBucketPolicy"
        ],
        "Effect": "Allow",
        "Resource": "arn:aws:s3:::nedarita"
      },
      {
        "Sid": "AccessS3Console",
        "Action": [
          "s3:GetBucketLocation",
          "s3:ListAllMyBuckets"
        ],
        "Effect": "Allow",
        "Resource": "arn:aws:s3:::*"
      }
    ]
  }